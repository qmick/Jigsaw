<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="scut.homework.jigsaw"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="scut.ftt.jigsaw.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name="scut.ftt.jigsaw.SelectActivity"
            android:label="@string/app_select"
            android:parentActivityName="scut.ftt.jigsaw.MainActivity"
            >                       
        </activity>
        <activity 
            android:name="scut.ftt.jigsaw.GameActivity"
            android:label="@string/app_game"
            android:parentActivityName="scut.ftt.jigsaw.SelectActivity"
            >           
        </activity>
    </application>

</manifest>
